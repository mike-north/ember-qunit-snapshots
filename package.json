{
  "name": "ember-qunit-snapshots",
  "version": "0.0.0-development",
  "description": "QUnit snapshot testing for ember.js apps",
  "keywords": [
    "ember-addon"
  ],
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/mike-north/ember-qunit-snapshots.git"
  },
  "author": {
    "email": "michael.l.north@gmail.com",
    "name": "Mike North",
    "url": "https://mike.works"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:ts": "tslint --project .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "ci-info": "^2.0.0",
    "ember-cli-babel": "^7.1.3",
    "ember-cli-typescript": "^2.0.0-beta.3",
    "fs-extra": "^8.0.0",
    "json-typescript": "^1.0.1",
    "qunit-snapshot": "^1.3.0",
    "snap-shot-core": "^8.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.0.0",
    "@commitlint/config-conventional": "8.0.0",
    "@commitlint/travis-cli": "8.0.0",
    "@ember/optional-features": "0.7.0",
    "@mike-north/js-lib-renovate-config": "1.1.5",
    "@mike-north/js-lib-semantic-release-config": "1.0.1",
    "@types/body-parser": "1.17.1",
    "@types/ember": "3.1.1",
    "@types/ember-qunit": "3.4.7",
    "@types/ember-test-helpers": "1.0.6",
    "@types/ember-testing-helpers": "0.0.3",
    "@types/ember__test-helpers": "0.7.9",
    "@types/express": "4.17.1",
    "@types/fs-extra": "7.0.0",
    "@types/qunit": "2.9.0",
    "@types/rsvp": "4.0.3",
    "broccoli-asset-rev": "3.0.0",
    "ember-ajax": "5.0.0",
    "ember-cli": "3.13.1",
    "ember-cli-code-coverage": "1.0.0-beta.8",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-htmlbars": "4.0.5",
    "ember-cli-htmlbars-inline-precompile": "3.0.1",
    "ember-cli-inject-live-reload": "2.0.2",
    "ember-cli-qunit": "4.4.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-template-lint": "1.0.0-beta.3",
    "ember-cli-tslint": "0.1.4",
    "ember-cli-typescript-blueprints": "2.0.0",
    "ember-cli-uglify": "3.0.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.0",
    "ember-load-initializers": "2.1.0",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-resolver": "5.3.0",
    "ember-source": "3.13.3",
    "ember-source-channel-url": "2.0.1",
    "ember-try": "1.2.1",
    "eslint-plugin-ember": "7.2.0",
    "eslint-plugin-node": "10.0.0",
    "husky": "2.4.1",
    "loader.js": "4.7.0",
    "qunit-dom": "0.9.0",
    "semantic-release": "15.12.5",
    "ts-node": "8.0.3",
    "tslint": "5.17.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-sonarts": "1.9.0",
    "typescript": "3.5.1"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "./node_modules/.bin/commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "extends": "@mike-north/js-lib-semantic-release-config"
  },
  "toolchain": {
    "node": "10.13.0",
    "yarn": "1.10.0"
  }
}
